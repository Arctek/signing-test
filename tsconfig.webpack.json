{
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "declaration": false,
        "experimentalDecorators": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "moduleResolution": "node",
        "module": "commonjs",
